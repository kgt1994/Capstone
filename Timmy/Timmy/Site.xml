<?xml version="1.0" encoding="utf-8" ?>
<sqlMap 
  namespace="Site" 
  xmlns="http://ibatis.apache.org/mapping"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >
  
  <alias>
    <typeAlias alias="Site"  type="Timmy.Site, Timmy" />
  </alias>

  <resultMaps>
    <resultMap id="SiteResultMap" class="Site">
      <result column="site_name" property="사이트이름"/>
      <result column="site_link" property="링크"/>
    </resultMap>
  </resultMaps>
  
  <statements>
    
    <select id="SelectSite"  resultMap="SiteResultMap">
      select site_name, site_link from site;
    </select>
  
  </statements>
  
</sqlMap>